module NucleotideCount (..) where

import String


nucleotideCounts : String -> { a : Int, t : Int, c : Int, g : Int }
nucleotideCounts sequence =
  let
    getCount n =
      String.filter (\c -> c == n) sequence |> String.length
  in
    { a = getCount 'A'
    , t = getCount 'T'
    , c = getCount 'C'
    , g = getCount 'G'
    }
